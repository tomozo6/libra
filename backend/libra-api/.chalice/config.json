{
  "version": "2.0",
  "app_name": "libra-api",
  "api_gateway_endpoint_type": "REGIONAL",
  "stages": {
    "dev": {
      "api_gateway_stage": "api",
      "environment_variables": {
        "PAY_TABLE_NAME": "libra-dev-pay-dynamodb-table"
      },
      "autogen_policy": false
    },
    "beta": {
      "api_gateway_stage": "api",
      "environment_variables": {
        "PAY_TABLE_NAME": "libra-beta-pay-dynamodb-table"
      },
      "autogen_policy": false
    },
    "prod": {
      "api_gateway_stage": "api",
      "environment_variables": {
        "PAY_TABLE_NAME": "libra-prod-pay-dynamodb-table"
      },
      "autogen_policy": false
    }
  }
}